#include <stdio.h>
#include <stdbool.h>

int main() {
    int arr[] = {10, 25, 8, 42, 17, 33};
    int n = sizeof(arr) / sizeof(arr[0]);
    int target = 42;
    int index = -1;
    
    for (int i = 0; i < n; i++) {
        if (arr[i] == target) {
            index = i;
            break;
        }
    }
    
    if (index != -1) {
        printf("Element %d found at index %d.\n", target, index);
    } else {
        printf("Element %d not found in the array.\n", target);
    }
    
    return 0;
}
