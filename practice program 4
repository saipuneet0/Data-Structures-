#include <stdio.h>

int main() {
    int arr[] = {4, 2, 5, 2, 6, 4, 8, 5};
    int n = sizeof(arr) / sizeof(arr[0]);

    printf("Duplicate elements in the array are: ");

    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (arr[i] == arr[j]) {
                printf("%d ", arr[i]);
                break;
            }
        }
    }

    printf("\n");

    return 0;
}
