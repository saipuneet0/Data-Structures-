#include <stdio.h>
#include <string.h>

void sortString(char str[]) {
    int i, j;
    int n = strlen(str);
    char temp;
    
    for (i = 0; i < n - 1; i++) {
        for (j = 0; j < n - i - 1; j++) {
            if (str[j] > str[j + 1]) {
                temp = str[j];
                str[j] = str[j + 1];
                str[j + 1] = temp;
            }
        }
    }
}

int main() {
    char str[] = "programming";
    
    printf("Original string: %s\n", str);
    
    sortString(str);
    
    printf("Sorted string: %s\n", str);
    
    return 0;
}

