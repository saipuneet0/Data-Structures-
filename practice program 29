#include <stdio.h>

int main() {
    int arr[] = {1, 2, 2, 3, 4, 2, 3, 3, 3, 5};
    int n = sizeof(arr) / sizeof(arr[0]);
    int max_freq_count = 0;
    
    for (int i = 0; i < n; i++) {
        int current_count = 0;
        for (int j = 0; j < n; j++) {
            if (arr[i] == arr[j]) {
                current_count++;
            }
        }
        if (current_count > max_freq_count) {
            max_freq_count = current_count;
        }
    }
    
    printf("The count of the most frequently repeated number is: %d\n", max_freq_count);
    
    return 0;
}
